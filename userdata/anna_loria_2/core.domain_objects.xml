	<rule id='45244b3938bc4bf69eba4e333f649752'>
		<name>Thermostat presets</name>
		<description>Provides presets for a Location.</description>
		<template id="d972c5bfff4c47649013f4ac7e58c274" tag="zone_setpoint_and_state_based_on_preset"/>
		<active>true</active>
		<created_date>2023-11-09T11:28:42.532+01:00</created_date>
		<modified_date>2025-03-07T19:13:27.904+01:00</modified_date>
		<deleted_date></deleted_date>
		<directives>
			<when preset="home"><then heating_setpoint="20" cooling_setpoint="22"/></when>
			<when preset="away"><then heating_setpoint="15" cooling_setpoint="25"/></when>
			<when preset="vacation"><then heating_setpoint="15" cooling_setpoint="28"/></when>
			<when preset="no_frost"><then heating_setpoint="10" cooling_setpoint="30"/></when>
			<when preset="asleep"><then heating_setpoint="19" cooling_setpoint="24"/></when>
		</directives>
		<contexts>
			<context>
				<zone><location id="c6afc5b4eba44c5eb912a9528cb5a3f2"/></zone>
			</context>
		</contexts>
	</rule>
	<template id='6a688fd0825c4a1cb9a5d1fcbb541ae4' tag="zone_preset_based_on_time_and_presence_with_override">
		<name>Zone preset schedule template</name>
		<description>Template for scheduling presets on a presence or time basis with the option to override the preset&apos;s setpoint/state.</description>
		<single_actor>true</single_actor>
		<created_date>2023-11-09T11:28:36.308+01:00</created_date>
		<modified_date>2024-01-17T14:01:24.771+01:00</modified_date>
		<deleted_date></deleted_date>
		<objects>
			<object name="zone" type="Location"/>
		</objects>
		<parameters>
			<parameter name="presence" type="Boolean" source="zone.presence"/>
			<parameter name="time" type="WeekTime" source="clock.weekTime"/>
		</parameters>
		<results>
			<result name="setpoint" action="zone.iterateFunctionalities('ThermostatFunctionality', 'thermostat')().setSetpoint(clock.addSeconds(null, 1), setpoint, setpoint)" type="Number"/>
			<result name="state" action="zone.iterateFunctionalities('RelayFunctionality')().switch(clock.addSeconds(null, 1), state)" type="OnOff"/>
			<result name="preset" action="zone.preset = preset" type="Preset"/>
			<result name="domestic_hot_water_state" action="zone.iterateFunctionalities('ToggleFunctionality', 'domestic_hot_water_state')().switch(clock.addSeconds(null, 1), domestic_hot_water_state)" type="OnOff"/>
		</results>
	</template>
